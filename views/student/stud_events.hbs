<section>
  <div class="row">
    <div class="col-md-2">
      <nav class="navbar navbar-expand-lg navbar-light">

        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav"
          aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
          <div class="navbar-nav">
            <div class="sidebar">
              <a href="/student_home">Home</a>
              <a href="/student_profile">Profile</a>
              <a href="/student_todays_task">Today's Task</a>
              <a href="/student_attendance/select_month">Attendance</a>
              <a href="/student_assignments">Assignments</a>
              <a href="/student_notes">Notes</a>
              <a href="/student_announcements">Announcements</a>
              <a class="active" href="/student_events">Events</a>
              <a href="/student_photos">Photos</a>

            </div>
          </div>
        </div>
      </nav>
    </div>

    <div class="col-md-8 event content m-auto">
      <h2 class="text-center">Events</h2>
      <table class="table-striped table-bordered">
        <thead>
          <tr>
            <th style="width: 40%;" class="pl-2">Date</th>
            <th class="pl-2">Event</th>

          </tr>
        </thead>
        <tbody>
          {{#each event}}
          <tr>
            <td class="pl-2"><a href="/each_event/{{this._id}}">{{this.date}}</a></td>
            <td class="pl-2"><a href="/each_event/{{this._id}}">{{this.event}}</a></td>
          </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
</section>